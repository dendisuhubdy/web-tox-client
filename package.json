{
  "name": "tox-rs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "@types/websocket": "0.0.40",
    "core-js": "^2.6.9",
    "dexie": "^2.0.4",
    "lzma": "^2.3.2",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.10",
    "vue-chat-scroll": "^1.3.5",
    "vue-class-component": "^7.1.0",
    "vue-draggable-resizable": "^2.0.0-rc2",
    "vue-material": "^1.0.0-beta-11",
    "vue-native-notification": "^1.0.5",
    "vue-property-decorator": "^8.1.1",
    "vue-qrcode-component": "^2.1.1",
    "vue-qrcode-reader": "^2.0.1",
    "vue-router": "^3.0.6",
    "vue2-touch-events": "^2.0.0",
    "vuex": "^3.1.1",
    "ws-tox-protocol": "github:tox-rs/ws-tox-protocol"
  },
  "devDependencies": {
    "@types/jest": "^23.1.4",
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-e2e-cypress": "^3.8.0",
    "@vue/cli-plugin-pwa": "^3.8.0",
    "@vue/cli-plugin-typescript": "^3.8.1",
    "@vue/cli-plugin-unit-jest": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "ts-jest": "^23.0.0",
    "typescript": "^3.4.4",
    "vue-template-compiler": "^2.5.21"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "watchPlugins": [
      "/home/redhat/projects/tox-rs/node_modules/jest-watch-typeahead/filename.js",
      "/home/redhat/projects/tox-rs/node_modules/jest-watch-typeahead/testname.js"
    ],
    "globals": {
      "ts-jest": {
        "babelConfig": true
      }
    }
  }
}
